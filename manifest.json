{
    "name": "RickBlocker",
    "description": "An extension designed to block rickrolls.",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}